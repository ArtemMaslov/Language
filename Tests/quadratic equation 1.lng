main()
{
	a = 0
	b = 0
	c = 0
	
	in a
	in b
	in c
	
	SolveQuadraticEquation(a, b, c)
}

SolveQuadraticEquation(a, b, c)
{
	if (a == 0)
	{
		if (b == 0)
		{
			if (c == 0)
			{
				out -1
			}
			else
			{
				out 0
			}
		}
		else
		{
			out 1
			out -c / b
		}
	}
	else
	{
		discr = b * b - 4 * a * c
		
		if (discr > 0)
		{
			discr = sqrt(discr)
			out 2
			out (0 - b + discr) / (2 * a)
			out (0 - b - discr) / (2 * a)
		}
		else 
		{
			if (discr == 0)
			{
				out 1
				out (0 - b) / (2 * a)
			}
			else
			{
				out 0
			}
		}
	}
}